package lec02;

public class IfLogic01 {
	public static void main(String[] args) {
		
		// if
		/*
		 * if (true) { System.err.println("true"); } if (false) {
		 * System.err.println("false"); }
		 */
		
		int paramA = 1;
		if (paramA == 1) {
			System.out.println(1);
		} else if (paramA == 2) {
			System.out.println(2);
		}
		
		
		
	}
}
