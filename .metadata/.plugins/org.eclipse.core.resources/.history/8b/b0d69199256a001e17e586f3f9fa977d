package lec01;

import java.util.ArrayList;

public class Array05 {
	public static void main(String[] args) {
		
		// 배열(array) 선언
		// 방법1
		int [] arrA = new int[]{1,2,3,4,5};
		
		// 방법2
		int [] arrB = new int[5];
		arrB[0] = 1;
		arrB[2] = 2;
		arrB[3] = 3;
		arrB[4] = 4;
		arrB[5] = 5;
		
		ArrayList<Integer> sampleList = new ArrayList();
		sampleList.add(1);
		sampleList.add(2);
		
	
	}
}
