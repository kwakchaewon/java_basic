package lec01;

public class PrimitiveType04 {
	public static void main(String[] args) {
		
		// boolean: 참거짓 타입
		boolean isTrue = false;
		
		// char: 문자열 타입
		char oneChar = '가';
		char twoChar='나';
		
		// 숫자형 타입
		byte firstByte = 1;
		short secondByte = 2;
		int third = 3;
		long forth = 4;
		
		int frontData = 0;
		int backData = 3;
		int multipleData = frontData + backData;

		System.out.println("multipleData: " + multipleData);
		
	}
}
