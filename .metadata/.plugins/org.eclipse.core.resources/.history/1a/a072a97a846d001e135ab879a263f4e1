package lec11;

public class MainMethod {
	public static void main(String[] args) {
		System.out.println("메인메소드 실행"); // 메인메소드 실행
		
		ExtendsClass exntenClass = new ExtendsClass();
		exntenClass.methodSecond(); // 상속받은 클래스에서 로직이 더 들어갑니다 / 추상클래스 methodSecond
		int thirdResult = exntenClass.methodThirds();
		System.out.println("세번째값입니다 ::: " + thirdResult);
		
		
		// 1) "메인메소드 실행" 출력
		// 2) "상속받은 클래스에서 로직이 더 들어갑니다" 출력
		// 3) "추상클래스 methodSecond" 출력
		// 4) "상속받은 클래스의 오버라이딩된 methodFirst"
		// 5) 24 출력
	}
}
