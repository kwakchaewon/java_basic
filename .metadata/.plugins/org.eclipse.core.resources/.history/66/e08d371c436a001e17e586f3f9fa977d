package lec02;

public class CaseLogic02 {
	public static void main(String[] args) {
		// switch-case는 각종 케이스를 통해 조건을 걸러줌
		int paramA = 1;
		
		switch (paramA) {
		case 1:			
			System.out.println('1입니다');

		case 2:
			System.out.println('2입니다');
			
		case 3:			
			System.out.println();
			
		case 4:			
			System.out.println();
		}
		
	}
}
