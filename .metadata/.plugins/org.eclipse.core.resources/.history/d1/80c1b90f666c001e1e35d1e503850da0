package lec11;

public class MainMethod {
	public static void main(String[] args) {
		System.out.println("메인메소드 실행");
		
		ExtendsClass exntenClass = new ExtendsClass();
		exntenClass.methodSecond();
		int thirdResult = exntenClass.methodThirds();
		System.out.println("세번째값입니다 ::: " + thirdResult);
		
		// 1) "상속받은 클래스에서 로직이 더 들어갑니다" 출력
		// 2) "추상클래스 methodSecond" 출력
		
	}
}
