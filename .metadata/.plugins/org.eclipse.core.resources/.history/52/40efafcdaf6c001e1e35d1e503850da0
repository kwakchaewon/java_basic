package lec14.collectionframework;

import java.util.ArrayList;

public class ArrayLIist03 {
	
	public static void main(String[] args) {
		ArrayList sampleList = new ArrayList();
		
		sampleList.add(1);
		sampleList.add("한국");
		sampleList.add(true);
		sampleList.add("안녕");
		
		System.out.println(sampleList.toString());
		
		
		
		
	}

}
